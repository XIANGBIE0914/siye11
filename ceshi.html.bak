<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="Generator" content="EditPlus®">
  <meta name="Author" content="">
  <meta name="Keywords" content="">
  <meta name="Description" content="">
  <title>Document</title>
 </head>
 <body>
    <style>
  * {margin:0;
      padding:0;
	  }

  .p1{background-color:rgb(22,187,157);
      width:100%;
	  height:794px;
	  }
   .key {
		/* url 代表指定具体的哪一张图片 */
		background-image: url(key.png);

		/* width: 100px; */

		width: 94px;
		height: 20px;
	transform: translate(14px, 44px);
	}

	.new .key {
		animation: typing 1.5s steps(5) 1.3s forwards;
	}

	/* 定义1个动画，名字叫 typing, 规定了从哪里开始，到什么时候结束 */	
	@keyframes typing {
		from {
			width: 0;
		}

		to {
			width: 100px;
		}
	}

	
.search {
	width: 529px;
	height: 320px;
	

	padding: 200px;
}

.input {
	width: 529px;
	height: 66px;
	background-image: url(search.png);

	transform: translate(-800px, 0);
}


.new .input {
	transition: all 1s ease-out 1s;
	transform: translate(0, 0);
}
 
 .result{width: 100%;
	height: 111%;
	background: url(result.png);
	transform: translateY(-200%);
	}
 .wrap {
	width: 529px;
	height: 393px;
	transform: translateY(-13px);
	
    }
.new.result{ 
	transition: all 0.5s ease-in-out 2.8s;
	transform: translateY(0);}

	.info {
	width: 612px;
	height: 299px;
	background: url(info_4.png);
	display:block;
   float:right;
}



 </style>
 <div class="p1">

<div class="search">
  <div class="key"></div>
  <div class="input"></div>
  <div class="result"></div>
  <div class="wrap"></div>
  <div class="info"></div>
  </div>

</div>

 
 <script>
  setTimeout(function(){$(".key").addClass("new");

	}, 100);
</script>
 </body>
</html>
